import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("================================");
        while (sc.hasNext()) {
            String str = sc.next(); 
            int num = sc.nextInt(); 
              System.out.printf("%-15s%03d%n", str, num);
        }
        System.out.println("================================");
        sc.close();
    }
}
